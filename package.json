    "react-draggable": "^3.0.5",
    "react-flexbox-grid": "^2.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transform-hmr": "1.0.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "3.0.1",
    "sass-loader": "^7.0.1",
    "sinon": "2.3.7",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "winston": "^2.4.2",
    "xunit-file": "1.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
