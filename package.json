    "koa-webpack-hot-middleware": "1.0.3",
    "lodash": "4.17.4",
    "material-ui": "^1.0.0-beta.27",
    "measured": "1.1.0",
    "minimist": "1.2.0",
    "mocha": "3.4.2",
    "moment": "2.18.1",
