{
  "name": "smash_bracket",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "description": "node microapp for smash_bracket",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node util/es6_mode --file src/server/server.js",
    "predev": "npm run clean",
    "dev": "NODE_ENV=dev nodemon --inspect util/es6_mode --file src/server/server.js",
    "clean": "rm -rf dist && npm prune",
    "build": "webpack --config util/webpack/prod.js ",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "XUNIT_FILE=./test_results/unit.xml mocha --opts test/unit/mocha.opts",
    "test:unit:dev": "mocha --opts test/unit/mocha_dev.opts",
    "pretest:integration": "babel test/integration/index._test.js --out-file integration_tests.js",
    "test:integration": "casperjs test integration_tests.js --xunit=test_results/integration.xml",
    "lint": "eslint --cache true --ext .jsx,.js ./src/**",
    "lint:fix": "eslint --ext .jsx,.js --fix ./src/**"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.6",
    "app-module-path": "2.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "^6.26.0",
    "casperjs": "1.1.4",
    "chai": "4.1.0",
    "config": "^1.30.0",
    "css-loader": "^0.28.11",
    "enzyme": "2.9.1",
    "epimetheus": "1.0.50",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^1.1.11",
    "git-rev": "0.2.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.6.1",
    "handlebars": "4.0.10",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.10.0",
    "json-loader": "^0.5.7",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-ejs": "^4.1.1",
    "koa-json": "2.0.2",
    "koa-mount": "3.0.0",
    "koa-proxies": "^0.7.0",
    "koa-render": "0.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "1.0.3",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.27",
    "material-ui-pickers": "^1.0.0-beta.12",
    "measured": "1.1.0",
    "minimist": "1.2.0",
    "mocha": "3.4.2",
